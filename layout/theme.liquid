  <body class="{{template}} {{template | handle}} {% if template contains 'landing' or template contains 'collection' or template contains 'fb-welcomeback-new' %}page-landing{% endif %} {% if template contains 'fb-welcomeback-new' %} collection {% endif %}  prestige--v4 {% if template.directory %}template-{{ template.directory | handle }}{% endif %} template-{{ template.name | handle }}">
    <a class="PageSkipLink u-visually-hidden" href="#main">{{ 'header.navigation.skip_to_content' | t }}</a>
    <span class="LoadingBar"></span>
    <div class="PageOverlay"></div>
    <div class="PageTransition"></div>
    {% section 'popup' %}
    {% section 'sidebar-menu' %}

    {%- if template != 'cart' -%}
      {% include 'cart-drawer' %}
    {%- endif -%}

    <div class="PageContainer">
      {% unless template contains 'landing' %}
      {% section 'announcement' %}
      {% endunless %}
      {% section 'header' %}

      <main id="main" role="main">
        {{ content_for_layout }}
      </main>

      {% section 'footer' %}
    </div>

    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-M3B2KTJ" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->
	{% include 'subscription-theme-footer' %}
    
    <!-- Ipstack -->
    {% if template contains 'product' %}
    	{% include 'ipstack-script' %}
    {% endif %}
